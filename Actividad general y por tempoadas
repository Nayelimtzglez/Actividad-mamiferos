####Patrón de actividad por temporadas####
base <- read_csv("Estadística circular .csv")

####TEMPORADAS####

# ARMADILLO Lluvia#
armadillo_lluvia<-subset(armadillo, armadillo$Temporada=="Lluvia")
data.frame(table(armadillo_lluvia$Temporada))

arma_circular_lluvia<-circular(armadillo_lluvia$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(arma_circular_lluvia,5)

#ARMADILLO Seca#
armadillo_seca<-subset(armadillo, armadillo$Temporada=="Seca")
data.frame(table(armadillo_seca$Temporada))

arma_circular_seca<-circular(armadillo_seca$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(arma_circular_seca,5)

#COATÍ Lluvia#
coati_lluvia<-subset(coati, coati$Temporada=="Lluvia")
data.frame(table(coati_lluvia$Temporada))

coati_circular_lluvia<-circular(coati_lluvia$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(coati_circular_lluvia,5)

#COATÍ Seca#
