#Watson test para dos muestras#
watson.two.test(arma_circular_lluvia, arma_circular_seca, alpha=0.05)
watson.two.test(coati_circular_lluvia, coati_circular_seca, alpha=0.05)
watson.two.test(conejo_circular_lluvia, conejo_circular_seca, alpha=0.05)
watson.two.test(jaguar_circular_lluvia, jaguar_circular_seca, alpha=0.05)
watson.two.test(ocelote_circular_lluvia, ocelote_circular_seca, alpha=0.05)
watson.two.test(osonegro_circular_lluvia, osonegro_circular_seca, alpha=0.05)
watson.two.test(pecari_circular_lluvia, pecari_circular_seca, alpha=0.05)
watson.two.test(puma_circular_lluvia,puma_circular_seca, alpha=0.05)
watson.two.test(temazaterojo_circular_lluvia, temazaterojo_circular_seca, alpha=0.05)
watson.two.test(tepezcuintle_circular_lluvia, tepezcuintle_circular_seca, alpha=0.05)
watson.two.test(tigrillo_circular_lluvia, tigrillo_circular_seca, alpha=0.05)
watson.two.test(tlacuache_circular_lluvia, tlacuache_circular_seca, alpha=0.05)
watson.two.test(venado_circular_lluvia,venado_circular_seca, alpha=0.05)
watson.two.test(viejodemonte_circular_lluvia, viejodemonte_circular_seca, alpha=0.05)
watson.two.test(zorragris_circular_lluvia, zorragris_circular_seca, alpha=0.05)
watson.two.test(zorrillo_circular_lluvia, zorrillo_circular_seca, alpha=0.05)

#Gráficas#
registers<-read.csv("Estadística circular .csv")
registers$Tiempo<-as.character(registers$Tiempo)
registers$Horas_Decimales<-sapply(strsplit(registers$Tiempo,":"), function(x){
  x<-as.numeric(x)
  x[1]+x[2]/60+x[3]/3600
}
)

registers$Temporada<-as.character(registers$Temporada)
view(registers)

#ARMADILLO LLUVIA#
armadillo1<-registers%>% filter(Especie=="Dasypus novemcinctus")
armadillo_lluvia<-subset(armadillo1, armadillo1$Temporada=="Lluvia")


ggplot(armadillo_lluvia,aes(x=Horas_Decimales))+
  geom_histogram(breaks=seq(0,24), fill="steelblue4",colour="black",size=0.3)+
  scale_x_continuous("", limits = c(0,24),breaks=seq(0,24), labels=seq(0,24))+
  annotate("rect", xmin = c(18,0), xmax = c(24,8),ymin=0, ymax=15,alpha=0.3,fill="grey25")+
  annotate("rect", xmin=6, xmax=20, ymin=0, ymax=15, alpha=0.3, fill="#FFD819")+
  labs(title = "Registros de *Dasypus novemcinctus*",y="Número de registros")+
  coord_polar(start=0)+
  theme_minimal()+
  theme(text=element_text(size=13, face="bold"), axis.title.x = element_text(margin = unit(c(2,0,0,0), "mm")),axis.title.y = element_text(margin=unit(c(0,3,0,0), "mm")), plot.title = element_markdown(hjust = 0.5))

plo1<-ggplot(armadillo_lluvia,aes(x=Horas_Decimales))+
  geom_histogram(breaks=seq(0,24), fill="steelblue4",colour="black",size=0.3)+
  scale_x_continuous("", limits = c(0,24),breaks=seq(0,24), labels=seq(0,24))+
  annotate("rect", xmin = c(18,0), xmax = c(24,8),ymin=0, ymax=15,alpha=0.3,fill="grey25")+
  annotate("rect", xmin=6, xmax=20, ymin=0, ymax=15, alpha=0.3, fill="#FFD819")+
  labs(title = "Registros de *Dasypus novemcinctus*",y="Número de registros")+
  coord_polar(start=0)+
  theme_minimal()+
  theme(text=element_text(size=13, face="bold"), axis.title.x = element_text(margin = unit(c(2,0,0,0), "mm")),axis.title.y = element_text(margin=unit(c(0,3,0,0), "mm")), plot.title = element_markdown(hjust = 0.5))

dasypus<-readPNG("Dasypus.png")
grafica1<-ggdraw(plo1)+
  draw_image(sun,x=0.50,y=0.18,
             width = 0.07, height = 0.06,scale=0.70)+
  draw_image(moon,x=0.55, y=0.76,
             width = 0.08, height=0.07, 
             scale=0.52)+
  draw_image(dasypus, x=0.25, y=0.82,
             width= 0.08, height = 0.08,
             scale=1.2)

grafica1


