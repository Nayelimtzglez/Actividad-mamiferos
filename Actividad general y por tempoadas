#ANÁLISIS DE LA ACTIVIDAD: PATRÓN GENERAL Y POR TEMPORADAS#
#TOMADO DE SALVADOR MANDUJANO Y PEREZ-SOLANO#
#GRÁFICAS DE GABRIEL ANDRADE-PONCE#

library(CircStats)
library(MASS)
library(boot)
library(circular)
library(overlap)
library(ggplot2)
library(readr)

base <- read_csv("Estadística circular .csv")
head(base)
####Registros de cada organismo####
armadillo<- subset(base, base$Especie=="Dasypus novemcinctus")
coati<- subset(base, base$Especie=="Nasua narica")
conejo<-subset(base, base$Especie=="Sylvilagus floridanus")
jaguar<-subset(base, base$Especie=="Panthera onca")
ocelote<-subset(base, base$Especie=="Leopardus pardalis")
osonegro<-subset(base, base$Especie=="Ursus americanus")
puma<-subset(base, base$Especie=="Puma concolor")
temazaterojo<-subset(base, base$Especie=="Mazama temama")
tepezcuintle<-subset(base, base$Especie=="Cuniculus paca")
tigrillo<-subset(base, base$Especie=="Leopardus wiedii")
tlacuache<-subset(base, base$Especie=="Didelphis virginiana")
viejodemonte<-subset(base, base$Especie=="Eira barbara")
zorragris<-subset(base, base$Especie=="Urocyon cinereoargenteus")
zorrillo<-subset(base, base$Especie=="Conepatus leuconotus")

####Objeto circular####
arma_circular<-circular(armadillo$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(arma_circular,5)
coa_circular<-circular(coati$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(coa_circular,5)
cone_circular<-circular(conejo$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(cone_circular,5)
jagu_circular<-circular(jaguar$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(jagu_circular,5)
oce_circular<-circular(ocelote$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(oce_circular,5)
oso_circular<-circular(osonegro$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(oso_circular,5)
peca_circular<-circular(pecaridecollar$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(peca_circular,5)
puma_circular<-circular(puma$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(puma_circular,5)
tema_circular<-circular(temazaterojo$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(tema_circular,5)
tepe_circular<-circular(tepezcuintle$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(tepe_circular,5)
tigr_circular<-circular(tigrillo$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(tigr_circular,5)
tlacu_circular<-circular(tlacuache$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(tlacu_circular,5)
vena_circular<-circular(venadocolablanca$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(vena_circular,5)
viejo_circular<-circular(viejodemonte$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(viejo_circular,5)
zorra_circular<-circular(zorragris$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(zorra_circular,5)
zorrillo_circular<-circular(zorrillo$Horas_Decimales,units="hour",template="clock24",rotation="clock",zero=pi/2)
head(zorrillo_circular,5)

####Estadística inferencial####
#Rayleigh test#
rayleigh.test(arma_circular)
rayleigh.test(coa_circular)
rayleigh.test(cone_circular)
rayleigh.test(jagu_circular)
rayleigh.test(oce_circular)
rayleigh.test(oso_circular)
rayleigh.test(peca_circular)
rayleigh.test(puma_circular)
rayleigh.test(tema_circular)
rayleigh.test(tepe_circular)
rayleigh.test(tigr_circular)
rayleigh.test(tlacu_circular)
rayleigh.test(vena_circular)
rayleigh.test(viejo_circular)
rayleigh.test(zorra_circular)
rayleigh.test(zorrillo_circular)

#Rao´s spacing test#
rao.spacing.test(arma_circular, alpha=0.05)
rao.spacing.test(coa_circular, alpha=0.05)
rao.spacing.test(cone_circular, alpha=0.05)
rao.spacing.test(jagu_circular, alpha=0.05)
rao.spacing.test(oce_circular, alpha=0.05)
rao.spacing.test(oso_circular, alpha=0.05)
rao.spacing.test(peca_circular, alpha=0.05)
rao.spacing.test(puma_circular, alpha=0.05)
rao.spacing.test(tema_circular, alpha=0.05)
rao.spacing.test(tepe_circular, alpha=0.05)
rao.spacing.test(tigr_circular, alpha=0.05)
rao.spacing.test(tlacu_circular, alpha=0.05)
rao.spacing.test(vena_circular, alpha=0.05)
rao.spacing.test(viejo_circular, alpha=0.05)
rao.spacing.test(zorra_circular, alpha=0.05)
rao.spacing.test(zorrillo_circular, alpha=0.05)

####Gráficas####
library(tidyverse)
library(ggtext)
library(rphylopic)
library(cowplot)
library(png)
library(overlap)
library(ggplot2)
library(magick)

registers<-read.csv("Estadística circular .csv")

registers$Tiempo<-as.character(registers$Tiempo)
registers$Horas_Decimales<-sapply(strsplit(registers$Tiempo,":"), function(x){
  x<-as.numeric(x)
  x[1]+x[2]/60+x[3]/3600
}
)

####Patrón de actividad general####
registers<-read.csv("Estadística circular .csv")

registers$Tiempo<-as.character(registers$Tiempo)
registers$Horas_Decimales<-sapply(strsplit(registers$Tiempo,":"), function(x){
  x<-as.numeric(x)
  x[1]+x[2]/60+x[3]/3600
}
)

#ARMADILLO#
armadillo1<-registers%>% filter(Especie=="Dasypus novemcinctus")

ggplot(armadillo1,aes(x=Horas_Decimales))+
  geom_histogram(breaks=seq(0,24), fill="steelblue4",colour="black",size=0.3)+
  scale_x_continuous("", limits = c(0,24),breaks=seq(0,24), labels=seq(0,24))+
  annotate("rect", xmin = c(18,0), xmax = c(24,8),ymin=0, ymax=16,alpha=0.3,fill="grey25")+
  annotate("rect", xmin=6, xmax=20, ymin=0, ymax=16, alpha=0.3, fill="#FFD819")+
  labs(title = "Registros de *Dasypus novemcinctus*",y="Número de registros")+
  coord_polar(start=0)+
  theme_minimal()+
  theme(text=element_text(size=13, face="bold"), axis.title.x = element_text(margin = unit(c(2,0,0,0), "mm")),axis.title.y = element_text(margin=unit(c(0,3,0,0), "mm")), plot.title = element_markdown(hjust = 0.5))

plot33<-ggplot(armadillo1,aes(x=Horas_Decimales))+
  geom_histogram(breaks=seq(0,24), fill="steelblue4",colour="black",size=0.3)+
  scale_x_continuous("", limits = c(0,24),breaks=seq(0,24), labels=seq(0,24))+
  annotate("rect", xmin = c(18,0), xmax = c(24,8),ymin=0, ymax=16,alpha=0.3,fill="grey25")+
  annotate("rect", xmin=6, xmax=20, ymin=0, ymax=16, alpha=0.3, fill="#FFD819")+
  labs(title = "Registros de *Dasypus novemcinctus*",y="Número de registros")+
  coord_polar(start=0)+
  theme_minimal()+
  theme(text=element_text(size=13, face="bold"), axis.title.x = element_text(margin = unit(c(2,0,0,0), "mm")),axis.title.y = element_text(margin=unit(c(0,3,0,0), "mm")), plot.title = element_markdown(hjust = 0.5))

sun <-readPNG("sun.png")
moon<-readPNG("moon.png")
dasypus<-readPNG("Dasypus.png")
grafica33<-ggdraw(plot33)+
  draw_image(sun,x=0.50,y=0.18,
             width = 0.07, height = 0.06,scale=0.70)+
  draw_image(moon,x=0.55, y=0.76,
             width = 0.08, height=0.07, 
             scale=0.52)+
  draw_image(dasypus, x=0.25, y=0.82,
             width= 0.08, height = 0.08,
             scale=1.2)

grafica33

